<div class="container">
  <h1>Restaurants::Chefs#Index</h1>
  <hr>

  <% @chefs.each do |chef|  %>
    <div class="card">
      <p> <%= chef.name %> restaurant ID: <%= chef.restaurant_id %> <%= link_to "Edit", edit_restaurants_chef_path(chef), class: "btn btn-primary" %> <%= link_to "Delete", restaurants_chef_path(chef), method: :delete, class: "btn btn-danger"  %></p>
      <p> <%= chef.profile %> </p>

    </div>
  <% end %>

  <%= link_to "Back", root_path %>
  <br> <br>
</div>
